{
  "expo": {
    "name": "AccentParlez",
    "slug": "AccentParlez",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.AccentParlez"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.AccentParlez"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "flipper": false
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "fcc0187d-8d59-477e-8a9f-e0bf3b73b83c"
      }
    },
    "owner": "hyunsoo_so"
  }
}
